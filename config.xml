<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10"
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Parked Washed</name>
    
    <description>
        Waterless car wash service on the go
    </description>
    <icon src="img/login-w-icon.png" />
    <author href="https://github.com/montagnefrank/" email="burton@burtonservers.com">
        Burton Technology
    </author>
    <gap:plugin name="cordova-plugin-geolocation" source="npm" />
    <gap:plugin name="cordova-plugin-whitelist" version="1.1.0" source="npm" />
    <access origin="*" launch-external="yes" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
</widget>